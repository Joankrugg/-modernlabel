<div class="index-wallpapers">
  <div class="container search">
    <div class="page-title-search">
      MUSIC MATCH SALLES
    </div>
    <%= render 'shared/place_search' %>
  </div>
  <div class="index-page">
    <div class="container">
      <div class="row">
        <% if @places.present? %>
          <% @places.each do |a|%>
            <div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
              <div class="main">
                <div class="card">
                  <%= render "place", place: a %><br>
                </div>
              </div>
            </div>
          <% end %>
        <% else %>
          <%= render 'shared/no_result' %>
        <% end %>
      </div>
    </div>
  </div>
</div>
