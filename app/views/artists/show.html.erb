<% content_for(:title_tag) do %>
  -<%= @artist.name %>-
<% end %>
<% content_for(:description) do %>
  <%= @artist.description %>
<% end %>
<div class="negative top">
  <div class="search search-fixed">
    <%= form_tag(artists_path, method: :get) do %>
      <%= text_field_tag :search, nil, placeholder: "Search artists by name, city, genre or number", class: "form-control" %>
      <%= submit_tag 'Search', style: "display: none;" %>
    <% end %>
  </div>
</div>
<%= render 'shared/artistcard'  %>

