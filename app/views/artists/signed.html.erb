<div class="container">
  <div class="page-title-search" style="color: black;">
    MODERN BOX ARTISTES
  </div>
</div>

<div class="banner">
  <div class="container banner-content">
    <% @artists.each do |artist|%>
      <div class="col-xs-6 col-sm-6 col-md-3 col-lg-3">
        <div class="id-card">
          <%= link_to artist do %>
            <%= cl_image_tag artist.photo, height: 230, width: 250, crop: :fill, class: "id-pic img-responsive" %>
          <% end %>
          <div class="details"><%= artist.name %></div>
        </div>
      </div>
    <% end %>
  </div>
</div>


