<%= render 'shared/artist_search' %>
<div class="banner">
  <div class="banner-content">
    <!--section d'ensemble-->
    <div class="container items">
      <div class="row">
        <% @artists.each do |a|%>
          <div class="col-xs-6 col-sm-4 col-md-3 col-lg-3">
            <div class="main">
              <div class="card">
                <%= render "artist", artist: a %><br>
              </div>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>

